            </div>
            <div className="flex items-center gap-1 text-gray-700">
              <Scale size={12} />
              <span>{weight}</span>
            </div>
          </div>
        </div>
      );
    }

    if (department === 'GYNA') {
      return (
        <div className="space-y-2">
